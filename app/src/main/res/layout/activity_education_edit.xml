<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
     <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:orientation="vertical" >

        <include
            android:id="@+id/public_top_bar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            layout="@layout/public_layout_topbar" />
        
        <TableLayout
        android:layout_marginTop="10dip"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:stretchColumns="*" >

        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="5dip"
            android:background="@drawable/btn_layout_list_two" >
<LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                
                >
                <TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="*"
    android:textColor="#FF0000"
    android:layout_marginTop="7dip"
    ></TextView>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="left|center_vertical"
                android:paddingLeft="10dp"
                android:layout_marginTop="6dip"
                android:textSize="18sp"
                android:textColor="#5B5B5B"
                android:text="@string/starttime" />
</LinearLayout>
            <TextView
                android:id="@+id/education_starttime_id"
                android:layout_height="wrap_content"
                android:background="@drawable/input_shape"
                android:drawableRight="@drawable/icon_arrow"
                android:gravity="right"
                android:textSize="16sp"
                android:ems="10"
                android:textColor="@android:color/darker_gray"
                 android:text="请输入"
                android:clickable="true"
                />
        </TableRow>
        
        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="5dip"
            android:background="@drawable/btn_layout_list_two" >
<LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                
                >
                <TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="*"
    android:textColor="#FF0000"
    android:layout_marginTop="7dip"
    ></TextView>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#5B5B5B"
                android:layout_marginTop="7dip"
                android:layout_gravity="left|center_vertical"
                android:paddingLeft="10dp"
                android:textSize="18sp"
                android:text="@string/endtime" />
</LinearLayout>
            <TextView
                android:id="@+id/education_endtime_id"
                android:layout_height="wrap_content"
                android:background="@drawable/input_shape"
                android:drawableRight="@drawable/icon_arrow"
                android:gravity="right"
                android:ems="10"
                android:clickable="true"
                android:textColor="@android:color/darker_gray"
                 android:text="请输入"
               android:textSize="16sp"
                />
        </TableRow>
        
            
        <TableRow
            android:layout_width="wrap_content"
            android:padding="5dip"
            android:layout_height="wrap_content"
            android:background="@drawable/btn_layout_list_two" >
<LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                
                >
                <TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="*"
    android:textColor="#FF0000"
    android:layout_marginTop="7dip"
    ></TextView>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#5B5B5B"
                android:layout_marginTop="7dip"
                android:layout_gravity="left|center_vertical"
                android:paddingLeft="10dp"
                android:textSize="18sp"
                android:text="@string/school" />
</LinearLayout>
            <EditText
                android:id="@+id/education_school_id"
                android:layout_height="wrap_content"
                android:background="@drawable/input_shape"
                android:drawableRight="@drawable/icon_arrow"
                android:ems="10"
                android:gravity="right"
                android:singleLine="true"
                  android:hint="请输入"
                  android:textSize="16sp"
                android:textColor="@android:color/darker_gray"
                />
        </TableRow>
        
        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="5dip"
            android:background="@drawable/btn_layout_list_two" >
<LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                
                >
                <TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="*"
    android:textColor="#FF0000"
    android:layout_marginTop="7dip"
    ></TextView>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="left|center_vertical"
                android:paddingLeft="10dp"
                android:layout_marginTop="7dip"
                android:textSize="18sp"
                android:textColor="#5B5B5B"
                android:text="@string/educationbg" />
</LinearLayout>
            <TextView
                android:id="@+id/education_educationbg_id"
                android:layout_height="wrap_content"
                android:background="@drawable/input_shape"
                android:drawableRight="@drawable/icon_arrow"
                android:gravity="right"
                android:hint="@string/educationbg_hit"
                android:tag="DICT_JOB_EDU"
                android:textColor="@android:color/darker_gray"
                android:textSize="16sp"
                android:ems="10"
                />
        </TableRow>
        
        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="5dip"
            android:background="@drawable/btn_layout_list_two" >
<LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                
                >
                <TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="*"
    android:textColor="#FF0000"
    android:layout_marginTop="7dip"
    ></TextView>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="left|center_vertical"
                android:paddingLeft="10dp"
                android:padding="5dip"
                android:layout_marginTop="6dip"
                android:textSize="18sp"
                
                android:textColor="#5B5B5B"
                android:text="@string/specialtyid" />
</LinearLayout>
            <EditText
                android:id="@+id/education_specialtyid_id"
                android:layout_height="wrap_content"
                android:background="@drawable/input_shape"
                android:drawableRight="@drawable/icon_arrow"
                android:ems="10"
                android:textColor="@android:color/darker_gray"
                android:textSize="16sp"
                android:hint="请输入"
                android:singleLine="true"
                android:gravity="right"
                />
        </TableRow>
        
        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="5dip"
            android:background="@drawable/btn_layout_list_two" >
<LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                
                >
                <TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="*"
    android:textColor="#FF0000"
    android:layout_marginTop="7dip"
    ></TextView>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="left|center_vertical"
                android:paddingLeft="10dp"
                android:layout_marginTop="7dip"
                android:textColor="#5B5B5B"
                android:textSize="18sp"
                android:text="@string/specialtyremark" />
</LinearLayout>
            <TextView
                android:id="@+id/education_specialtyremark_id"
                android:layout_height="wrap_content"
                android:background="@drawable/input_shape"
                android:drawableRight="@drawable/icon_arrow"
                android:gravity="right"
                  android:text="请输入"
                  android:textSize="16sp"
                android:textColor="@android:color/darker_gray"
                android:tag="@string/specialtyremark"
                android:ems="10"
                />
        </TableRow>
        
        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="5dip"
            android:background="@drawable/btn_layout_list_two" >
<LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                
                >
                <TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="*"
    android:textColor="#FF0000"
    android:layout_marginTop="7dip"
    ></TextView>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="left|center_vertical"
                android:paddingLeft="10dp"
                android:textSize="18sp"
                android:layout_marginTop="7dip"
                android:textColor="#5B5B5B"
                android:text="@string/studyabroad" />
</LinearLayout>
            <TextView
                android:id="@+id/education_studyabroad_id"
                android:layout_height="wrap_content"
                android:background="@drawable/input_shape"
                android:drawableRight="@drawable/icon_arrow"
                android:gravity="right"
                  android:text="请选择"
                android:textColor="@android:color/darker_gray"
                android:textSize="16sp"
                android:tag="DICT_RESUME_STUDYH"
                android:ems="10"
                />
        </TableRow>
        
        
        </TableLayout>
        </LinearLayout>

</LinearLayout>
