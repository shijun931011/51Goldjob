<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    
 <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:orientation="vertical" >

        <include
            android:id="@+id/public_top_bar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            layout="@layout/public_layout_topbar" />
        
        <TableLayout
        android:layout_marginTop="10dip"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:stretchColumns="*" >

        
        <TableRow
            android:layout_width="wrap_content"
            android:padding="7dip"
            android:layout_height="wrap_content"
            android:background="@drawable/btn_layout_list_two" >
            
             <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                
                >
                <TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="*"
    android:textColor="#FF0000"
    android:layout_marginTop="7dip"
    ></TextView>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="left|center_vertical"
                android:paddingLeft="10dp"
                android:layout_marginTop="7dip"
                android:textColor="#5B5B5B"
                android:textSize="18sp"
                android:text="@string/language_type" />
</LinearLayout>
            <TextView
                android:id="@+id/language_type_id"
                android:layout_height="wrap_content"
                android:background="@drawable/input_shape"
                android:drawableRight="@drawable/icon_arrow"
                android:gravity="right"
                android:paddingRight="15dip"
                android:textColor="#5B5B5B"
                android:textSize="16sp"
                android:hint="@string/language_type_hit"
                android:tag="DICT_RESUME_LANTYPE"
                android:ems="10"
                />
        </TableRow>
        
        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="7dip"
            android:background="@drawable/btn_layout_list_two" >
 <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                
                >
                <TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="*"
    android:textColor="#FF0000"
    android:layout_marginTop="7dip"
    ></TextView>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="left|center_vertical"
                android:paddingLeft="10dp"
                android:textColor="#5B5B5B"
                android:textSize="18sp"
                  android:layout_marginTop="7dip"
                android:text="@string/language_mastery" />
</LinearLayout>
            <TextView
                android:id="@+id/language_mastery_id"
                android:layout_height="wrap_content"
                android:background="@drawable/input_shape"
                android:drawableRight="@drawable/icon_arrow"
                android:gravity="right"
                android:paddingRight="15dip"
                android:hint="@string/language_mastery_hit"
                android:tag="DICT_RESUME_MASTERY"	
                android:ems="10"
                android:textColor="#5B5B5B"
                android:textSize="16sp"
                android:clickable="true"
                />
        </TableRow>
        
        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="7dip"
            android:background="@drawable/btn_layout_list_two" >
 <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                
                >
                <TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="*"
    android:textColor="#FF0000"
    android:layout_marginTop="7dip"
    ></TextView>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="left|center_vertical"
                android:paddingLeft="10dp"
                android:textColor="#5B5B5B"
                android:textSize="18sp"
                  android:layout_marginTop="7dip"
                android:text="@string/language_randw" />
</LinearLayout>
           	<TextView
                android:id="@+id/language_randw_id"
                android:layout_height="wrap_content"
                android:background="@drawable/input_shape"
                android:drawableRight="@drawable/icon_arrow"
                android:gravity="right"
                android:layout_marginRight="5dip"
                android:hint="@string/language_randw_hit"
                android:tag="DICT_RESUME_MASTERY"	
                android:ems="10"
                android:textColor="#5B5B5B"
                android:textSize="16sp"
                android:clickable="true"
                />
        </TableRow>

        
         <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="7dip"
            android:background="@drawable/btn_layout_list_two" >
 <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                
                >
                <TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="*"
    android:textColor="#FF0000"
    android:layout_marginTop="7dip"
    ></TextView>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="left|center_vertical"
                android:paddingLeft="10dp"
                android:textColor="#5B5B5B"
                android:textSize="18sp"
                  android:layout_marginTop="7dip"
                android:text="@string/language_lands" />
</LinearLayout>
            <TextView
                android:id="@+id/language_lands_id"
                android:layout_height="wrap_content"
                android:background="@drawable/input_shape"
                android:drawableRight="@drawable/icon_arrow"
                android:gravity="right"
                android:textSize="16sp"
                android:paddingRight="15dip"
                android:textColor="#5B5B5B"
                android:hint="@string/language_lands_hit"
                android:tag="DICT_RESUME_MASTERY"	
                android:ems="10"
                />
        </TableRow>
        
        
    </TableLayout>
    </LinearLayout>
</LinearLayout>
